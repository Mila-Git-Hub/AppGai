<Page x:Class="AppGai.HistoryCarPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AppGai"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="HistoryCarPage">

    <Grid Background="#b4c7e7">
        <DataGrid x:Name="historyTable" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="ФИО" Width="*" Binding="{Binding fio}"/>
                <DataGridTextColumn Header="Date Start Having" Width="*" Binding="{Binding dateStartHaving}"/>
                <DataGridTextColumn Header="Date End Having" Width="*" Binding="{Binding dateEndhaving}"/>
                
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Page>
